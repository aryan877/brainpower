{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": []
    },
    "clean": {
      "cache": false
    }
  },
  "globalPassThroughEnv": ["NODE_ENV", "PORT"]
}
